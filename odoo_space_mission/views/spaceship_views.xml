<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="action_view_spaceship" model="ir.actions.act_window">
        <field name="name">Odoo Spaceships</field>
        <field name="res_model">space.ship</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smilling_face">
                Create your first spaceship model.
            </p>
        </field>
    </record>

    <menuitem id="space_mission_main_menu_"
              sequence="1"
              name="Odoo Space Mission"/>

    <menuitem id="space_mission_sub_1"
              sequence="1"
              name="Odoo Space Mission"
              parent="space_mission_main_menu"/>

    <menuitem id="space_mission_spaceships"
              sequence="1"
              name="Spaceships"
              parent="space_mission_sub_1"
              action="action_view_spaceship"/>
</odoo>